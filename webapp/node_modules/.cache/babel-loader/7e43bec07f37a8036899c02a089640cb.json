        <h1 class=\"name\" id=\"name\"></h1>\n                        <p class=\"temp\"></p>\n                        <p class=\"clouds\"></p>\n                        <p class=\"desc\"></p>\n                </div>\n                   \n              \n\n\n\n                <div class=\"btn-1\">\n                    <div class=\"d-flex justify-content-around\">\n                        <button type=\"button\" class=\"btn btn-primary btn-lg btn1\">\n                            <h1 class=\"click\">Click here to<br>Start your<br>journey</h1>\n                        </button>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n\n    <div class=\"container\">\n        <div class=\"flex\">\n            <form class=\"mt-5\"><br><br><br>\n                <div class=\"form-group\">\n                    <label for=\"email\"><h3>From:</h3></label>\n                    <input type=\"input\" id=\"from\" class=\"form-control form-control-lg\">\n                    <label for=\"email\"><h3>To:</h3></label>\n                    <input type=\"input\" id=\"to\" class=\"form-control form-control-lg\">\n                </div>\n\n                <button class=\"btn btn-secondary btn-lg btn-block btn1\">\n                    <h1>Go-&#62;</h1>\n                </button>\n                <button class=\"btn btn-secondary btn-lg btn-block btn2\">\n                    <h1>&#60;-back</h1>\n                </button><br><br><br><br><br>\n            </form>\n        </div>\n    </div>\n\n\n    <div class=\"container\">\n        <div class=\"flex\"><br><br><br>\n            <h1 class=\"lower1\">What type of route would you like?</h1><br><br>\n            <button type=\"button\" class=\"btn btn-primary btn-lg btn-block btn1\">\n                <h2> Fastest </h2>\n            </button><br><br>\n\n            <button type=\"button\" class=\"btn btn-primary btn-lg btn-block btn1\">\n                <h2> Most convenient </h2>\n            </button><br><br>\n            \n            <button class=\"btn btn-secondary btn-lg btn-block btn2\">\n                <h1>&#60;-back</h1>\n            </button><br><br><br><br>\n        </div>\n    </div>\n\n\n    <div class=\"container\" style=\"margin-bottom: 3%;\">\n        <div class=\"flex\"><br><br>\n            <h1 Class=\"lower1\">Your route is:</h1>\n            <div class=\"iframe\">\n                <iframe \n                    src=\"https://www.openstreetmap.org/export/embed.html?bbox=24.93942886590958%2C60.169588787253716%2C24.941247403621677%2C60.17128187292611&amp;layer=mapnik&amp;marker=60.17043534099665%2C24.940338134765625\" \n                    style=\"border: 1px solid black; width: 100ch; height: 100ch;\">\n                </iframe><br>\n                <button class=\"btn btn-secondary btn-lg btn-block btn1\">\n                    <h1>Finnish-&#62;</h1>\n                </button>\n                <button class=\"btn btn-secondary btn-lg btn-block btn2\">\n                    <h1>&#60;-back</h1>\n                </button><br><br>\n            </div>\n        </div>\n    </div>\n</div>\n</template>\n<script>\n\n  var input = document.querySelector('.input_text');\nvar main = document.querySelector('#name');\nvar temp = document.querySelector('.temp');\nvar desc = document.querySelector('.desc');\n//var clouds = document.querySelector('.clouds');\nvar button= document.querySelector('.submit');\n\n\n\nbutton.addEventListener('click', function(){\nfetch('https://api.openweathermap.org/data/2.5/weather?q='+input.value+'&appid=03fdcab4998339b7fb8504d1efd0e89f')\n.then(response => response.json())\n.then(data => {\n  var tempValue = data['main']['temp'];\n  var nameValue = data['name'];\n  var descValue = data['weather'][0]['description'];\n\n  main.innerHTML = nameValue;\n  desc.innerHTML = \"Desc - \"+descValue;\n  temp.innerHTML = \"Temp - \"+tempValue;\n  input.value =\"\";\n\n})\n\n\n//.catch(err => alert(\"Wrong city name!\"));\n})\n\n\n\n\nvar x = document.getElementById(\"demo\");\n\n\n\nfunction showPosition(position) {\n    x.innerHTML = \"Latitude: \" + position.coords.latitude + \n    \"<br>Longitude: \" + position.coords.longitude; \nvar lat = position.coords.latitude.toFixed(0);\nvar lon = position.coords.longitude.toFixed(0);\n\n    fetch('https://api.openweathermap.org/data/2.5/weather?lat='+lat+'&lon='+lon+'&appid=03fdcab4998339b7fb8504d1efd0e89f')\n    .then(response => response.json())\n    .then(data => {\n      var tempValue = data['main']['temp'];\n      var nameValue = data['name'];\n      var descValue = data['weather'][0]['description'];\n    \n      main.innerHTML = nameValue;\n      desc.innerHTML = \"Desc - \"+descValue;\n      temp.innerHTML = \"Temp - \"+tempValue;\n      \n    \n    })\n\n\n}\n\n</script>\n<script>\nexport default {\n    name: \"Header\"\n}\n</script>"],"sourceRoot":"src/components"}]}